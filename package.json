{
  "name": "camunda-modeler-plugin",
  "version": "0.2.0",
  "description": "Camunda modeler BPMN linter plugin",
  "main": "index.js",
  "scripts": {
    "all": "run-s bundle copy-assets",
    "copy-assets": "copyfiles --flat node_modules/bpmn-js-bpmnlint/dist/assets/css/bpmn-js-bpmnlint.css style/style.css dist",
    "bundle": "webpack",
    "bundle:watch": "webpack -w",
    "prepublishOnly": "run-s all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/onify/camunda-modeler-plugin"
  },
  "keywords": [
    "camunda",
    "modeler",
    "plugin"
  ],
  "author": "Onify",
  "license": "MIT",
  "devDependencies": {
    "bpmn-js-bpmnlint": "^0.17.0",
    "bpmnlint": "^7.2.1",
    "bpmnlint-loader": "^0.1.5",
    "bpmnlint-plugin-camunda": "^0.5.0",
    "bpmnlint-plugin-custom": "file:bpmnlint-plugin-custom",
    "camunda-modeler-plugin-helpers": "^3.1.0",
    "copyfiles": "^2.4.1",
    "min-dom": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.9.0"
  }
}
